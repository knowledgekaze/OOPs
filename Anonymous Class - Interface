//Program to understand the concept of Anonymous class

interface InterfDemo 
{ 
    int x = 21; 
    void show(); 
}
public class Main3 {
	public static void main(String[] args) { 
		
		InterfDemo i = new InterfDemo() { 
            @Override
            public void show() { 
                System.out.print("value is "+x); 
            } 
        }; 
        i.show(); 
    } 
}
