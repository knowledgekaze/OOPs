//Program to understand the concept of Anonymous class

abstract class Democlass {
	   public abstract void show();
	}

public class Main2 {
	 public static void main(String args[]) {
		 
		 Democlass d = new Democlass() {
	         public void show() {
	            System.out.println("anonymous class");
	         }
	        
	      };
	      d.show();
	   }

}
