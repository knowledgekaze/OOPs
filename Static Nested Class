//Program to understand the concept of Abstract class

class OuterCls 
{  
    static int x = 10;      
    int y = 20;      
    private static int z = 30; 
    
    static class StNestedCls //static nested class
    { 
         static void show() 
        { 
            System.out.println("x = " + x); 
            System.out.println("z = " + z); 


        } 
    } 
    
} 

public class Main {
		public static void main(String[] args) 
	    { 
//			OuterCls.StNestedCls obj = new OuterCls.StNestedCls(); 
//	        obj.show(); 
	        OuterCls.StNestedCls.show();
	    } 
	}
