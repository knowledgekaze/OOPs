//Program to expalain the Constructor 
public class Student{
	int roll_no;
	String name;
	String branch;
	
	//Default constr
	Student(){
		
	}
	//Parametrized constr
	Student(int r,String n,String b){
		roll_no=r;
		name=n;
		branch=b;
	}
		
	public static void main(String[] args) {
		Student s1=new Student();
		s1.roll_no=99;
		s1.name="Dekay";
		s1.branch="Mechanical";
		
		Student s2=new Student(101,"Auro","Computer Science");
		
		System.out.println(s1.roll_no+" "+s1.name+" "+s1.branch);
		System.out.println(s2.roll_no+" "+s2.name+" "+s2.branch);

	}

}
