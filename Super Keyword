//Program to understand the concept of Super Keyword
class A{
	int i=999;
	int j=1;
	public void fun(){
		System.out.println("class A method");
	}
}

class B extends A{
	int i=100;
	
	public void fun(){
		System.out.println("class B method");
	}
	public void display(){
		System.out.println(super.i);
		System.out.println(j);
		super.fun();
	}

}

public class Demo {
	public static void main(String[] args) {
		B b=new B();
		b.display();
	}

}
