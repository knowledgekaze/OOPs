//Program to understand the concept of Anonymous Class

class Demo {
	   public void show() {
	      System.out.println("Inside the Demo class");
	   }
	}

class Main {
	   public static void main(String[] args) {
		  Demo d=new Demo(){
			  public void show() {
			      System.out.println("Inside the Anonm class");
			   } 
		  };
		  d.show();
	   }
	}
