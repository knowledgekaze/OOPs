//Program to understand the concept of Encapsulation

class Emp{
	private String name;
	private int age;
	private int salary;
	//set methods
	public void setName(String n){
		name=n;
	}
	public void setAge(int a){
		age=a;
	}
	public void setSalary(int s){
		salary=s;
	}
	//get methods
	public String getName(){
		return name;
	}
	public int getAge(){
		return age;
	}
	public int getSalary(){
		return salary;
	}
}
public class EncapDemo {
	public static void main(String[] args) {
		
		Emp e1=new Emp();
		e1.setName("Dekay");
		e1.setAge(20);
		e1.setSalary(20000);
		
		System.out.println(e1.getName()+" "+e1.getAge()+" "+e1.getSalary());
		
	}

}
